{
    "name": "spryker/otel-elastica-instrumentation",
    "type": "library",
    "description": "OtelElasticaInstrumentation package",
    "license": "proprietary",
    "require": {
        "open-telemetry/api": "^1.0",
        "open-telemetry/context": "^1.0",
        "open-telemetry/sem-conv": "^1.0",
        "php": ">=8.1",
        "ruflin/elastica": "^7.0",
        "spryker/opentelemetry": "dev-feature/frw-8618/master-open-telemetry"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        },
        "files": [
            "_register.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Spryker",
            "homepage": "https://spryker.com"
        }
    ],
    "include-path": [
        "src/"
    ]
}
